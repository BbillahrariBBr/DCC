<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bakibillah.dcc">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_dcc"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_dcc"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CollegeHistoryActivity" />
        <activity android:name=".AdministratorActivity" />
        <activity android:name=".StudentInfoActivity" />
        <activity android:name=".CollegeInsfrastructureActivity" />
        <activity android:name=".CollegeLocationActivity" />
        <activity android:name=".AdmissionInformationActivity" />
        <activity android:name=".ContuctUsActivity" />
        <activity android:name=".RankingActivity" />
        <activity android:name=".FacultyActivity"></activity>
    </application>

</manifest>